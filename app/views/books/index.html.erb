<p id="notice"><%= notice %></p>

<%= link_to 'New Search', root_path %>

<h1>Listing Books</h1>

<table>
  <thead>
    <tr>
      <th>Call num</th>
      <th>Title</th>
      <th>Subtitle</th>
      <th>Author first</th>
      <th>Author last</th>
      <th>Copyright</th>
      <th>Subject</th>
      <th>Annotation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.Call_num %></td>
        <td><%= book.Title %></td>
        <td><%= book.Subtitle %></td>
        <td><%= book.Author_first %></td>
        <td><%= book.Author_last %></td>
        <td><%= book.Copyright %></td>
        <td><%= book.Subject %></td>
        <td><%= book.Annotation %></td>
        <td><%= link_to 'Show', book %></td>
        <td><%= link_to 'Edit', edit_book_path(book) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

